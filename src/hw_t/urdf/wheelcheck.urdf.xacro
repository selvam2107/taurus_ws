                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>

                <box size="0.0 0.0 0.0"/>
            </geometry>
        </collision>
    </link>
